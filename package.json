{
  "name": "bp-hapi-react-redux",
  "version": "1.0.7",
  "description": "Hapi React Redux Boilerplate",
  "main": "src/server.js",
  "scripts": {
    "test": "eslint . --ext .js, .jsx",
    "lint": "eslint . --ext .js,.jsx",
    "lint-fix": "eslint . --ext .js --fix",
    "build-server": "rimraf .build/ && babel ./src/server --out-dir .build/server/ --copy-files",
    "build-client": "webpack --mode production",
    "build": "npm run build-server && npm run build-client",
    "start-build": "NODE_ENV=localhost APP_MOCKS=1 node ./.build/server/server",
    "start-server": "NODE_ENV=localhost APP_MOCKS=1 APP_MODE_DEV=1 nodemon --exec babel-node src/server/server --ignore src/client",
    "start-client": "webpack-dev-server --mode development",
    "start": "node start.js",
    "docker-build": "./docker-build.sh $npm_package_name $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klishevich/bp-hapi-react-redux.git"
  },
  "author": "Michael Klishevich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klishevich/bp-hapi-react-redux/issues"
  },
  "homepage": "https://github.com/klishevich/bp-hapi-react-redux#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "h2o2": "^8.1.2",
    "handlebars": "^4.0.11",
    "hapi": "^17.3.1",
    "history": "^4.7.2",
    "inert": "^5.1.0",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.0",
    "nock": "^9.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "vision": "^5.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "nodemon": "^1.17.5",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
}
